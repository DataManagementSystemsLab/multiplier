@PACKAGE_INIT@

if(NOT TARGET @PROJECT_NAME@::tool)

  include(CMakeFindDependencyMacro)
  list(PREPEND CMAKE_MODULE_PATH "${CMAKE_CURRENT_LIST_DIR}")
  find_dependency(Filesystem)
  
  if(NOT TARGET gflags::gflags)
    find_dependency(gflags)
  endif()

  if(NOT TARGET glog::glog)
    find_dependency(glog)
  endif()
  
  # Dr. Lojekyll will bring in gRPC and Flatbuffers.
  if(NOT TARGET DrLojekyll::Runtime)
    find_dependency(DrLojekyll)
  endif()
  
  # PASTA will bring Z3, Clang, and LLVM.
  if(NOT TARGET pasta::pasta)
    find_dependency(pasta)
  endif()
  
  include("${CMAKE_CURRENT_LIST_DIR}/@PROJECT_NAME@Targets.cmake")
  check_required_components("@PROJECT_NAME@")

endif(NOT TARGET @PROJECT_NAME@::tool)
